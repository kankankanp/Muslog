{
  "extends": "next/core-web-vitals",
  "plugins": ["import", "unused-imports"],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        },
        "newlines-between": "never",
        "distinctGroup": false
      }
    ],
    "unused-imports/no-unused-imports": "error"
  }
}
